{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "plugins": ["react"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true
    }
  },
  "rules": {
    "camelcase": 0,
    "eol-last": 0,
    "quotes": [2, "single"],
    "new-cap": 0,
    "no-alert": 1,
    "no-console": 1,
    "no-debugger": 1,
    "no-trailing-spaces": 0,
    "no-underscore-dangle": 0,
    "no-unused-vars": 2,
    "semi": [2, "always"],
    "space-infix-ops": 0,
    "curly": [2, "all"]
  }
}